        {
            "name": "profile-loader",
            "type": "python",
            "request": "launch",
            "module": "cocmd_cli",
            "justMyCode": false,
            "args": ["profile-loader"]
        },
        {
            "name": "mdclick",
            "type": "python",
            "request": "launch",
